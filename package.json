{
  "name": "js-practice",
  "version": "1.0.0",
  "description": "Dis is da Js-Practice",
  "author": "noritersand",
  "private": true,
  "license": "UNLICENSED",
  "main": "none",
  "directories": {
    "test": "__tests__",
    "webRoot": "src/webroot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noritersand/js-practice.git"
  },
  "bugs": {
    "url": "https://github.com/noritersand/js-practice/issues"
  },
  "homepage": "https://github.com/noritersand/js-practice#readme",
  "scripts": {
    "start": "nodemon src/server.js",
    "test": "jest --config=__tests__/jest/jest.config.js --watchAll",
    "test:mocha": "mocha __tests__/mocha/**",
    "test:tap": "tap --bail --watch __tests__/tap/**",
    "build:jsdoc": "jsdoc src/jsdoc --destination jsdoc-out --configure src/jsdoc/jsdoc-config.json",
    "preview:jsdoc": "live-server jsdoc-out",
    "live": "live-server pages"
  },
  "devDependencies": {
    "chai": "^5.1.2",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "live-server": "^1.2.2",
    "mocha": "^10.8.2",
    "node-notifier": "^10.0.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "tap": "^21.0.1"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "express": "^4.21.1",
    "immer": "^10.1.1",
    "multer": "^1.4.5-lts.1",
    "sha3": "^2.1.4"
  },
  "type": "module",
  "packageManager": "yarn@4.5.2"
}
