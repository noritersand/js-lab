{
  "name": "js-testbed",
  "version": "1.0.0",
  "description": "Dis is da Js-Testbed",
  "main": "none",
  "directories": {
    "test": "src/test",
    "webRoot": "src/webRoot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noritersand/js-testbed.git"
  },
  "author": "",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/noritersand/js-testbed/issues"
  },
  "homepage": "https://github.com/noritersand/js-testbed#readme",
  "scripts": {
    "start": "nodemon server.js",
    "test": "mocha src/test/mocha/**",
    "tap": "tap --bail src/test/tap/**",
    "jsdoc": "jsdoc src/test/jsdoc --destination src/live-server/jsdoc-out --configure src/test/jsdoc/jsdoc-config.json"
  },
  "devDependencies": {
    "chai": "^4.3.3",
    "mocha": "^10.2.0",
    "tap": "^16.3.8",
    "jsdoc": "^4.0.2"
  },
  "dependencies": {
    "express": "^4.17.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.15",
    "sha3": "^2.1.4"
  }
}
