{
  "name": "js-testbed",
  "version": "1.0.0",
  "description": "Dis is da Js-Testbed",
  "main": "none",
  "directories": {
    "test": "tests",
    "webRoot": "src/webroot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noritersand/js-testbed.git"
  },
  "author": "",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/noritersand/js-testbed/issues"
  },
  "homepage": "https://github.com/noritersand/js-testbed#readme",
  "scripts": {
    "start": "nodemon src/server.js",
    "test": "jest --config=tests/jest/jest.config.js --watchAll",
    "mocha": "mocha --watch tests/mocha/**",
    "tap": "tap --bail --watch tests/tap/**",
    "jsdoc": "jsdoc src/jsdoc --destination jsdoc-out --configure src/jsdoc/jsdoc-config.json"
  },
  "devDependencies": {
    "chai": "^4.3.3",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "mocha": "^10.2.0",
    "node-notifier": "^10.0.1",
    "prettier": "^3.3.3",
    "tap": "^16.3.8"
  },
  "dependencies": {
    "express": "^4.17.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.15",
    "sha3": "^2.1.4"
  }
}
