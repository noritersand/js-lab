{
  "name": "js-practice",
  "version": "1.0.0",
  "description": "Dis is da Js-Practice",
  "author": "noritersand",
  "private": true,
  "license": "UNLICENSED",
  "main": "none",
  "directories": {
    "test": "__tests__",
    "webRoot": "src/webroot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noritersand/js-practice.git"
  },
  "bugs": {
    "url": "https://github.com/noritersand/js-practice/issues"
  },
  "homepage": "https://github.com/noritersand/js-practice#readme",
  "scripts": {
    "start": "nodemon src/server.js",
    "test": "jest --config=__tests__/jest/jest.config.js --watchAll",
    "mocha": "mocha --watch __tests__/mocha/**",
    "tap": "tap --bail --watch __tests__/tap/**",
    "jsdoc": "jsdoc src/jsdoc --destination jsdoc-out --configure src/jsdoc/jsdoc-config.json",
    "jsdoc-preview": "live-server jsdoc-out",
    "live": "live-server pages"
  },
  "devDependencies": {
    "chai": "^4.3.3",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "mocha": "^10.2.0",
    "node-notifier": "^10.0.1",
    "prettier": "^3.3.3",
    "tap": "^16.3.8"
  },
  "dependencies": {
    "dayjs": "^1.11.12",
    "express": "^4.17.2",
    "live-server": "^1.2.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.15",
    "sha3": "^2.1.4"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
